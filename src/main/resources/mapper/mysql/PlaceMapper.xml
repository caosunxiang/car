<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.car.mapper.mysql.PlaceMapper">
    <insert id="insertPlace">
insert into place (`name`,lat,lng,dic,createtime,createuser,`site`,phone,standbyphone,remark,standbyuser,starttime,endtime,area,video)
values (#{name},#{lat},#{lng},#{dic},#{createtime},#{createuser},#{site},#{phone},#{standbyphone},#{remark},#{standbyuser},#{starttime},#{endtime},#{area},#{video});
</insert>
    <select id="selectOne" resultType="com.example.car.entity.Place">
select * from place where id=#{id}
    </select>
    <select id="selectAll" resultType="com.example.car.entity.Place">
select * from place
    </select>
    <update id="updatePlace">
        update place set
         `name`=#{name},lat=#{lat},lng=#{lng},dic=#{dic},createtime=#{createtime},createuser=#{createuser},`site`=#{site},
         phone=#{phone},standbyphone=#{standbyphone},remark=#{remark},standbyuser=#{standbyuser},starttime=#{starttime},
         endtime=#{endtime},area=#{area},video=#{video}
         where
         id=#{id}
    </update>
    <delete id="delPlace">
delete from place where id=#{id}
    </delete>
</mapper>