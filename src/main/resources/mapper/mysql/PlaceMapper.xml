<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.car.mapper.mysql.PlaceMapper">
    <insert id="insertPlace">
insert into place (`name`,lat,lng,dic,createtime,createuser,`type`)
values (#{name},#{lat},#{lng},#{dic},#{createtime},#{createuser},#{type});
</insert>
    <select id="selectOne" resultType="com.example.car.entity.Place">
select * from place where id=#{id}
    </select>
    <select id="selectAll" resultType="com.example.car.entity.Place">
select * from place
    </select>
    <update id="updatePlace">
        update place set
         `name`=#{name},lat=#{lat},lng=#{lng},dic=#{dic},createtime=#{createtime},createuser=#{createuser},`type`=#{type}
         where
         id=#{id}
    </update>
    <delete id="delPlace">
delete from place where id=#{id}
    </delete>
</mapper>