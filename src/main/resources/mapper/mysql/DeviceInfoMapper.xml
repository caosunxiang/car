<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.car.mapper.mysql.DeviceInfoMapper">
<select id="selectDeviceInfo" resultType="com.example.car.entity.DeviceInfo">
    select d.*,c.carnumber from device_info d LEFT JOIN car_info c ON c.id=d.carid
</select>
</mapper>
